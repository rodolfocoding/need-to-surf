{
  "name": "need-to-surf",
  "version": "1.0.0",
  "description": "----------- Essa aplicação se reseume em uma API para calcular a melhor condição de surf entre varias praias.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only  src/index.ts ",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:functional": "jest --projects ./test --runInBand",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodolfocoding/need-to-surf.git"
  },
  "author": "rodolfocoding",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rodolfocoding/need-to-surf/issues"
  },
  "homepage": "https://github.com/rodolfocoding/need-to-surf#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "axios": "^0.26.1",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "module-alias": "^2.2.2",
    "mongoose": "^7.3.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.41",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.6.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
